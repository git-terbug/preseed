echo $HOME
echo $USER
mkdir /usr/local/etc/qwety
#ls -l ~/hola
mv $TOMCAT_APPS/geoportal/WEB-INF/classes/gpt/config/gpt.xml $TOMCAT_APPS/geoportal/WEB-INF/classes/gpt/config/gpt.xml.old
